{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": ["last 2 versions"]
      },
      "modules": false,
      "loose": true
    }],
    "@babel/preset-react",
    "@babel/preset-stage-2"
  ],
  "env": {
    "commonjs": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "browsers": ["last 2 versions"]
          },
          "modules": "commonjs",
          "loose": true,
          "useBuiltIns": "entry"
        }],
        "@babel/preset-stage-2"
      ]
    }
  }
}
